{
  "name": "yeditor-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng": "ng",
    "start": "npm run serve",
    "serve": "ng serve --host 127.0.0.1 --disable-host-check --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod  --app 1 --output-hashing=false",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "node dist/universal-prerender",
    "webpack:server": "webpack --config universal-webpack.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/universal-server",
    "codegen": "apollo-codegen introspect-schema http://127.0.0.1:4000/graphql --output api-schema.json && apollo-codegen generate --tag-name gql --schema api-schema.json --target typescript --output ./src/app/website/shared/services/api/graphql.data.ts ./src/app/**/graphql.service.ts"
  },
  "private": true,
  "dependencies": {
